/* pages/dl-manage/index.wxss */

.searchBox {
    width: 100%;
    height: 100rpx;
    padding-top: 20rpx;
    background: rgba(245, 245, 245, 1);
}

.seach {
    width: 690rpx;
    height: 80rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 8rpx;
    margin-left: 30rpx;
}

.fdj {
    width: 40rpx;
    height: 40rpx;
    margin-left: 30rpx;
    margin-top: 22rpx;
    font-size: 40rpx;
    line-height: 40rpx;
}

.seachInput {
    font-size: 36rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(205, 205, 205, 1);
    line-height: 50rpx;
}

/* 服务商列表 */

.dataBox {
    height: 100vh-100rpx;
    overflow: auto;
}

.dataList {
    width: 100%;
    height: auto;
}

.dataList-item {
    width: 720rpx;
    height: 134rpx;
    padding-top: 36rpx;
    padding-left: 30rpx;
    border-bottom: 1px solid #ededed;
}

.dataList-item-left {
    width: 500rpx;
    margin-right: 44rpx;
    display: inline-block;
}

.dataList-item-name {
    width: 100%;
    height: 44rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(51, 51, 51, 1);
    line-height: 44rpx;
    margin-bottom: 20rpx;
}

.dataList-item-time {
    width: 100%;
    height: 44rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(153, 153, 153, 1);
    line-height: 44rpx;
}
.dataList-item-right{
	display:inline-block;
	vertical-align:top;
}
.dataList-item-right-text {
	vertical-align:top;
    width: 104rpx;
    display: inline-block;
    font-size: 24rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(153, 153, 153, 1);
    line-height: 40rpx;
}
.you{
	width:40rpx;
	height:40rpx;
	vertical-align: top;
}
