/* 导航 */
.nav{
	width:750rpx;
	height:94rpx;
	position:relative;
	left:0;
	top:0;
	border-bottom:1px solid #E5E5E5
}
.nav-item{
	width:72rpx;
	line-height:94rpx;
	padding: 0 30rpx;
	font-size:36rpx;
	font-family:PingFangSC-Medium;
	font-weight:500;
	color:rgba(102,102,102,1);
	margin-right:35rpx;
	float:left;
}
.nav-item-active{
	width:72rpx;
	line-height:90rpx;
	font-size:36rpx;
	padding: 0 30rpx;
	font-family:PingFangSC-Medium;
	font-weight:500;
	margin-right:35rpx;
	float:left;
	color:rgba(61,172,106,1);
	border-bottom:3px solid rgba(61,172,106,1);
}
/* search */
.search{
	width:56rpx;
	height:56rpx;
	position:absolute;
	right:30rpx;
	top:20rpx;
}
.dataBox{
	width:750rpx;
	
	height:90vh;
	overflow:auto;
}
.dataList{
	height:auto;
}
.dataList-item{
	width:100%;
	height:162rpx;
	border-bottom:1px solid #EDEDED;
}
.dataList-item-top-name{
	margin-left:30rpx;
	margin-top:28rpx;
	margin-bottom:20rpx;
	display: inline-block;
	width:auto;
	height:44rpx;
font-size:32rpx;
font-family:PingFangSC-Medium;
font-weight:500;
color:rgba(51,51,51,1);
line-height:44rpx;
}
.dataList-item-top-type{
	display:inline-block;
	margin-left:20rpx;
	margin-top:28rpx;
	width:100rpx;
height:44rpx;
background:rgba(33,170,110,1);
border-radius:22rpx;
font-size:28rpx;
font-family:PingFangSC-Medium;
font-weight:500;
color:rgba(255,255,255,1);
line-height:44rpx;
text-align:center;
vertical-align:top;
}
.dataList-item-bottom{
	margin-left:30rpx;
	width:336rpx;
	height:44rpx;
	font-size:32rpx;
	font-family:PingFangSC-Medium;
	font-weight:500;
	color:rgba(153,153,153,1);
	line-height:44rpx;
}