.head {
    width: 750rpx;
    height: 88rpx;
    background: rgba(245, 245, 245, 1);
    font-size: 28rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(51, 51, 51, 1);
    line-height: 88rpx;
    text-indent: 30rpx;
}

.item-body {
    width: 100%;
    overflow: auto;
	margin-bottom:45px
}

.item {
    padding-top: 32rpx;
    border-bottom: 20rpx solid rgba(245, 245, 245, 1);
}

.item-head {
    width: 720rpx;
    margin-left: 30rpx;
    height: 36rpx;
    margin-bottom: 20rpx;
}

.biao {
    display: inline-block;
    width: 8rpx;
    height: 36rpx;
    background: rgba(61, 172, 106, 1);
    border-radius: 8rpx;
    vertical-align: top;
}

.title {
    display: inline-block;
    width: 300rpx;
    height: 36rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(51, 51, 51, 1);
    line-height: 36rpx;
    vertical-align: top;
    margin-left: 16rpx;
}

.item-content {
    width: 696rpx;
    height: 124rpx;
    margin-left: 54rpx;
    margin-top: 20rpx;
    margin-bottom: 24rpx;
}

.item-content-list {
    width: 200rpx;
    height: 124rpx;
    margin-right: 24rpx;
    float: left;
}

.item-content-list-name {
    width: 100%;
    height: 40rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(102, 102, 102, 1);
    line-height: 40rpx;
    text-align: center;
    margin-bottom: 12rpx;
}

.item-content-list-input {
    width: 100%;
    border-radius: 8px;
    border: 2px solid rgba(237, 237, 237, 1);
    height: 72rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(51, 51, 51, 1);
    line-height: 72rpx;
    text-align: center;
}

.sub {
    width: 100%;
    text-align: center;
    height: 100rpx;
    /* background: linear-gradient(360deg, rgba(61, 172, 106, 1) 0%, rgba(12, 186, 93, 1) 100%); */
	background:rgba(12, 186, 93, 1);
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
	z-index:9999
}
