/* 日期 */
.timeBox{
	width:100%;
	height:116rpx;
	background:white;
}
.chooseTime{
	width:450rpx;
height:76rpx;
border-radius:42px;
border:2px solid rgba(229,229,229,1);
margin-left:150rpx;
margin-top:20rpx;
}
.time{
	font-size:28rpx;
font-family:PingFangSC-Medium;
