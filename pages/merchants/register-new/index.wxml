                        <view class="picDivli">
                            <view class="picDivliimg">
                                <image src="{{shopData.bankCardPositive}}" id="bankCardPositive" data-type="6" mode="aspectFit" bindtap="chooseImage"></image>
                            </view>
                            <view class="picyulan" data-pic="6" catchtap="picmodel">查看示例</view>
                        </view>
                    </view>
                </view>
                <view class="picTips">*请上传真实有效的门店场景照片，保证图片清晰可见</view>
                <view class="picDivul">
                    <view class="picDivli">
                        <view class="picDivliimg">
                            <image src="{{shopData.doorheadPhoto}}" id="doorheadPhoto" data-type="5" mode="aspectFit" bindtap="chooseImage"></image>
                        </view>
                        <view class="picyulan" data-pic="7" catchtap="picmodel">查看示例</view>
                    </view>
                    <view class="picDivli">
                        <view class="picDivliimg">
                            <image src="{{shopData.cashier}}" id="cashier" data-type="13" mode="aspectFit" bindtap="chooseImage"></image>
                        </view>
                        <view class="picyulan" data-pic="8" catchtap="picmodel">查看示例</view>
                    </view>
                    <view class="picDivli">
                        <view class="picDivliimg">
                            <image src="{{shopData.placeBusiness}}" id="placeBusiness" data-type="14" mode="aspectFit" bindtap="chooseImage"></image>
                        </view>
                        <view class="picyulan" data-pic="9" catchtap="picmodel">查看示例</view>
                    </view>
                </view>
            </view>
        </view>
        <!-- 第四步 -->
        <view class="flxx" wx:if="{{Steps==3}}">
            <view class="line10"></view>
            <view class="xxblock huixian">
                <view class="huixianblock">
                    <view class="inplable hxlable">
                        <view>*</view>商户名称:
                    </view>
                    <view class="hxinput">{{shopData.merchantName}}</view>
                </view>
                <view class="line1"></view>
                <view class="huixianblock">
                    <view class="inplable hxlable">
                        <view>*</view>注册手机号:
                    </view>
                    <view class="hxinput">{{shopData.registerCell}}</view>
                </view>
            </view>
            <view class="line10"></view>
            <view class="xxblock">
                <view class="inputblock">
                    <view class="inplable">
                        <view>*</view>微信/支付宝费率(%)
                    </view>
                    <input class="inpinput {{input_active == 'rate'?'inpinput_a':''}}" type="digit" id="rate" value="{{waRate}}" bindinput="doInput" bindfocus="inputFocus" bindblur="inputBlur" placeholder="请填写支/微费率(示例0.38)" placeholder-class="input-placeholder"></input>
                    <view class="inpfun">
                        <image src="../../img/zhushi.png" class="tipsimg" mode="widthFix" catchtap="tipsmodel" data-tips="支付宝及微信费率,建议费率0.23-0.60之间"></image>
