                                <image src="{{shopData.holdId}}" id="holdId" data-type="11" mode="aspectFit" bindtap="chooseImage"></image>
                            </view>
                            <view class="picyulan" data-pic="5" catchtap="picmodel">查看示例</view>
                        </view>
                    </view>
                </view>
