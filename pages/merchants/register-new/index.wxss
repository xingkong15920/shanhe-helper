.inpinput {
    box-sizing: border-box;
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 8rpx;
    border: 2rpx solid rgba(229, 229, 229, 1);
    padding: 0 20rpx;
    padding-right: 100rpx;
    font-size: 32rpx;
    color: #333;
    transition: 0.35s all;
    position: relative;
    list-style: none;
    outline: none;
}

.inpinput::before, .inpinput::after {
    content: "";
    position: absolute;
    transition: 0.35s all;
    box-sizing: border-box;
    border-radius: 8rpx;
}

.inpinput::before {
    width: 0;
    height: 100%;
    top: 0;
    left: 50%;
    border-top: 2rpx solid rgba(229, 229, 229, 1);
    border-bottom: 2rpx solid rgba(229, 229, 229, 1);
}

.inpinput::after {
    width: 100%;
    height: 0;
    top: 50%;
    left: 0;
    border-left: 2rpx solid rgba(229, 229, 229, 1);
    border-right: 2rpx solid rgba(229, 229, 229, 1);
}

.inpinput.inpinput_a::before {
    width: 100%;
    left: 0;
    border-color: #fea600;
}

.inpinput.inpinput_a::after {
    height: 100%;
    top: 0;
    border-color: #fea600;
}

.inpinput.inpinput_f::before {
    width: 100%;
    left: 0;
    border-color: #fea600;
}

.inpinput.inpinput_f::after {
    height: 100%;
    top: 0;
    border-color: #fea600;
}

.xxblock .inputblock:last-child {
    margin: 0;
}

